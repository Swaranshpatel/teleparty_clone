{
  "manifest_version": 3,
  "name": "Watch Together",
  "version": "1.0",
  "permissions": ["scripting", "activeTab"],
  "host_permissions": ["<all_urls>"],
  "action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [{
    "matches": [
                "*://*.hotstar.com/*",
                "*://*.jiocinema.com/*"
                ],

    "js": ["content.js"]
  }]
}
